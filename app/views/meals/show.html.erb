Name: <%= @meal.name %><br>

<%= link_to 'Edit Meal', edit_meal_path(@meal) %>

 <ul>
 <%# byebug %>
    <li>Calories: <%# @meal.calculate_calories %></li>
    <li>Protein: <%= "45...fake" %></li>
    <li>Carbs: <%= "45...fake" %></li>
    <li>Fat: <%= "45...fake" %></li>
</ul>

