<%= render partial:'/shared/errors', locals: {obj: @foodlog} if @foodlog %>
<%= render partial:'/shared/errors', locals: {obj: @meal} if @meal %>

<%= form_for [@meal, @foodlog] do |f| %>
  <%= f.hidden_field :meal_id%>:
  <%= f.label :food_id, "Choose Food" %>:

  <%= f.collection_select(:food_id, @foods, :id, :name)%> 

  <%= f.number_field :quantity, in: 1..20, step: 1 %> <br>

  <%= f.submit %>
<% end %>